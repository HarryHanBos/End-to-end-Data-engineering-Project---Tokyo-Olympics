{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MoviesDB",
						"type": "DatasetReference"
					},
					"name": "MoviesDB"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filterYears"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movies as string,",
				"          Title as string,",
				"          genresgenregenre as string,",
				"          YEAR as string,",
				"          Rating as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 10,",
				"     ignoreNoFilesFound: false) ~> MoviesDB",
				"MoviesDB filter(toInteger(year) >= 1910 && toInteger(year) <= 2000 && rlike(genresgenregenre, 'Comedy')) ~> filterYears"
			]
		}
	}
}