{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tokyodatafactoryhh12"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1Tokyo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1Tokyo'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureDatabricksDeltaLake1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake1'"
		},
		"AzureFileStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureFileStorage1'"
		},
		"GitHub1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'GitHub1'"
		},
		"AzureDataLakeStorage1Tokyo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyodataharry12.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0618-195638-iym8jnb3"
		},
		"GitHub1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/HarryHanBos/End-to-end-Data-engineering-Project---Tokyo-Olympics.git"
		},
		"GitHub1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "harryhanbos"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tokyodatafactoryhh12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1Tokyo_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1Tokyo_accountKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricksDeltaLake1_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake1_accessToken')]"
					},
					"AzureFileStorage1_connectionString": {
						"value": "[parameters('AzureFileStorage1_connectionString')]"
					},
					"GitHub1_password": {
						"value": "[parameters('GitHub1_password')]"
					},
					"AzureDataLakeStorage1Tokyo_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1Tokyo_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"GitHub1_properties_typeProperties_url": {
						"value": "[parameters('GitHub1_properties_typeProperties_url')]"
					},
					"GitHub1_properties_typeProperties_userName": {
						"value": "[parameters('GitHub1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}